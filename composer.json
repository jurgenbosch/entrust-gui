{
    "name": "acoustep/entrust-gui",
    "description": "A GUI for the Entrust package.",
    "keywords": ["laravel","illuminate","auth","roles","acl","permission","gui"],
    "homepage": "https://github.com/acoustep/entrust-gui",
    "license": "MIT",
    "authors": [
        {
            "name": "Mitch Stanley",
            "email": "acoustep+entrustgui@fastmail.co.uk"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/console": "~5.5",
        "illuminate/support": "~5.5",
        "zizaco/entrust":  "~5.2",
        "prettus/l5-repository": "^2.6",
        "esensi/model": "~0.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "mockery/mockery": "0.9.*",
        "codeception/codeception": "2.4.*",
        "codeception/mockery-module": "*",
        "mikey179/vfsStream": "1.6.*",
        "symfony/Console": "3.3.*",
        "orchestra/testbench": "~3.4",
        "aedart/testing-laravel": "2.*"
    },
    "autoload": {
      "psr-4": {
          "Acoustep\\EntrustGui\\": "src/"
      }
    },
    "autoload-dev": {
        "psr-4": {
            "Acoustep\\EntrustGui\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
